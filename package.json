{
  "name": "cortex-debug-dp-efr32mg24",
	"displayName": "cortex-debug-dp-efr32mg24",
	"description": "This extension provides device support (SVD Definitions) for the SiliconLabs EFR32MG24 family",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:cortex-debug-dp-efr32mg24.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "cortex-debug-dp-efr32mg24.helloWorld",
            "title": "Hello World"

		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.46.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.11.7",
        "eslint": "^7.15.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.1.2",
        "vscode-test": "^1.4.1"
    }

}
